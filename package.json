{
  "name": "@enfo/sniffles",
  "version": "0.0.1",
  "description": "Automatic formatting and forwarding of CloudWatch logs based alarms in AWS using the CDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": {
    "name": "Enfo COPS",
    "email": "cops-aws@enfogroup.com"
  },
  "repository": {
    "url": "https://github.com/enfogroup/sniffles-cdk.git"
  },
  "scripts": {
    "prepublishOnly": "npx pinst --disable",
    "postpublish": "npx pinst --enable",
    "postinstall": "npx husky install",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -f lib/*.js lib/*.d.ts test/*.js test/*.d.ts",
    "test": "npm run clean && jest --runInBand --coverage",
    "lint": "npx eslint --ext .ts .",
    "lint:fix": "npx eslint --fix --ext .ts ."
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.3.0",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "^27.4.0",
    "@types/jspath": "^0.4.0",
    "@types/node": "^17.0.7",
    "@types/ramda": "^0.28.7",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "aws-cdk": "^2.3.0",
    "aws-cdk-lib": "^2.3.0",
    "aws-sdk": "^2.1108.0",
    "constructs": "^10.0.23",
    "esbuild": "^0.14.34",
    "eslint": "^8.6.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jsii": "^1.56.0",
    "jsii-pacmak": "^1.56.0",
    "pinst": "^3.0.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@enfo/aws-secrets": "^2.2.0",
    "@enfo/env-vars": "^1.0.0",
    "aws-lambda": "^1.0.7",
    "fp-ts": "^2.11.9",
    "jspath": "^0.4.0",
    "ramda": "^0.28.0"
  },
  "stability": "experimental",
  "jsii": {
    "outdir": ".dist",
    "versionFormat": "full"
  }
}
