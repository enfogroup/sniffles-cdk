{
  "name": "@enfo/sniffles",
  "version": "0.0.2",
  "description": "Automatic formatting and forwarding of CloudWatch logs based alarms in AWS using the CDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": {
    "name": "Enfo COPS",
    "email": "cops-aws@enfogroup.com"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/enfogroup/sniffles-cdk.git"
  },
  "scripts": {
    "prepublishOnly": "npx pinst --disable",
    "postpublish": "npx pinst --enable",
    "postinstall": "npx husky install",
    "build": "npx jsii",
    "build:watch": "jsii --watch",
    "package": "jsii-pacmak",
    "clean": "rm -f lib/*.js lib/*.d.ts lib/*.d.ts.map lib/opsGenie/*.js lib/opsGenie/*.d.ts lib/opsGenie/*.d.ts.map test/*.js test/*.d.ts test/*.d.ts.map",
    "test": "npm run clean && jest --runInBand --coverage",
    "lint": "npx eslint --ext .ts .",
    "lint:fix": "npx eslint --fix --ext .ts ."
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.3.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.382.0",
    "@aws-sdk/client-sns": "^3.382.0",
    "@aws-sdk/client-ssm": "^3.382.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "^29.5.3",
    "@types/jspath": "^0.4.0",
    "@types/node": "^20.4.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "aws-cdk": "^2.25.0",
    "aws-cdk-lib": "2.89.0",
    "constructs": "10.2.69",
    "esbuild": "^0.18.17",
    "eslint": "^8.6.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^29.6.2",
    "jsii": "^5.1.10",
    "jsii-pacmak": "^1.56.0",
    "pinst": "^3.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@enfo/aws-cdkompliance": "^1.1.0",
    "@enfo/aws-secrets": "^3.0.0",
    "@enfo/env-vars": "^1.0.0",
    "aws-lambda": "^1.0.7",
    "fp-ts": "^2.11.9",
    "jspath": "^0.4.0",
    "ramda": "^0.28.0"
  },
  "bundledDependencies": [
    "@enfo/aws-secrets",
    "@enfo/env-vars",
    "aws-lambda",
    "fp-ts",
    "jspath",
    "ramda"
  ],
  "peerDependencies": {
    "aws-cdk-lib": "^2.89.0",
    "constructs": "^10.2.69"
  },
  "stability": "experimental",
  "jsii": {
    "outdir": ".dist",
    "versionFormat": "full"
  }
}
